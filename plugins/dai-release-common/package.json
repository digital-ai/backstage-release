    "@backstage/plugin-permission-node": "^0.7.23"
  },
  "devDependencies": {
    "@backstage/cli": "^0.22.13",
    "@spotify/prettier-config": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
