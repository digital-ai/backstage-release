    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@mui/material": "^6.0.2",
    "@mui/styled-engine-sc": "^6.0.2",
    "@mui/x-date-pickers": "^7.16.0",
    "@types/lodash": "^4.17.7",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
